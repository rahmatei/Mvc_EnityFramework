@model IEnumerable<MVCEntityFrameWork.Models.Language>
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Index";
    Layout = "_Layout";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.LanguageName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.LanguageName)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.LanguageID">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.LanguageID">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.LanguageID">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
